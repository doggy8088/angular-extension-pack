{
  "name": "angular-extension-pack",
  "displayName": "Angular Extension Pack",
  "description": "Popular Visual Studio Code extensions for Angular Development",
  "version": "12.2.2",
  "publisher": "doggy8088",
  "icon": "icon.png",
  "repository": "https://github.com/doggy8088/angular-extension-pack",
  "engines": {
    "vscode": "^1.56.0"
  },
  "license": "MIT",
  "galleryBanner": {
    "color": "#DAD0C2",
    "theme": "light"
  },
